div class="row"
  h2 class="text-center"
    span class="label label-primary" 机器温度监控统计
  div class="col-md-offset-1 col-md-10"
    canvas id="canvas"

javascript:
  var fakeData = function(){ return Math.round(Math.random()*100)};
  var temperatureData = {
    labels: ["2015/12/19","2015/12/20","2015/12/21","2015/12/22","2015/12/23","2015/12/24","2015/12/25"],
    datasets: [{
      label: "Temperature Data",
      fillColor: "rgba(151,187,205,0.2)",
      strokeColor: "rgba(151,187,205,1)",
      pointColor: "rgba(151,187,205,1)",
      pointStrokeColor: "#fff",
      pointHighlightFill: "#fff",
      pointHighlightStroke: "rgba(151,187,205,1)",
      data: [fakeData(),fakeData(),fakeData(),fakeData(),fakeData(),fakeData(),fakeData()]
    }]
  }
  window.onload = function(){
    var ctx = document.getElementById("canvas").getContext("2d");
    window.myLine = new Chart(ctx).Line(temperatureData, { 
      responsive: true,
      scaleLabel: "<%=value%>℃",
      tooltipTemplate: "<%if (label){%><%=label%>: <%}%><%= value %>℃"
    });
  }
